{
    "builds": [{
        "src": "core/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "30mb", "runtime": "python3.9" }
    },{
        "src": "build_files.sh",
        "use": "@vercel/static",
        "config": {
          "distDir": "staticfiles"
        }
      }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "core/wsgi.py"
        }
    ]
}
